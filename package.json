{
  "name": "smol-range",
  "author": "Formidable",
  "version": "0.0.2",
  "description": "Small range utility function, like Python's but with some sugar on top.",
  "keywords": [
    "range"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/formidablelabs/smol-range.git"
  },
  "homepage": "https://github.com/FormidableLabs/smol-range",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": [
      {
        "import": "./dist/index.mjs",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./dist/index.js"
    ],
    "./package.json": "./package.json"
  },
  "scripts": {
    "test:watch": "vitest",
    "test": "vitest run",
    "build": "tsup",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "check:ci": "yarn run typecheck && yarn run lint && yarn run test",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "tsup": "^5.12.8",
    "typescript": "^4.6.4",
    "vitest": "^0.12.9"
  },
  "engines": {
    "node": ">= 14"
  }
}
